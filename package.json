{
  "name": "alice-cookie",
  "version": "1.0.0",
  "description": "Symbolブロックチェーン向けの署名スマホアプリ aLice を利用するためのユーティリティライブラリ",
  "keywords": [
    "symbol",
    "blockchain",
    "utility"
  ],
  "author": "harvestasya <x.com: @ccHarvestasya>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nemnesia/alice-cookie.git"
  },
  "homepage": "https://github.com/nemnesia/alice-cookie#readme",
  "bugs": {
    "url": "https://github.com/nemnesia/alice-cookie/issues"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "type": "module",
  "packageManager": "yarn@4.9.3",
  "scripts": {
    "build": "rollup -c",
    "lint": "yarn eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\"",
    "lint:fix": "yarn eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "format": "yarn prettier --check \"src/**/*.{ts,tsx}\"",
    "format:fix": "yarn prettier --write \"src/**/*.{ts,tsx}\""
  },
  "main": "dist/node/alice-cookie.mjs",
  "module": "dist/node/alice-cookie.mjs",
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/alice-cookie.mjs",
      "require": "./dist/umd/alice-cookie.umd.js"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.3",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "rollup": "^4.44.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "24.4.1",
    "npm": "11.4.2",
    "yarn": "4.9.3"
  }
}
